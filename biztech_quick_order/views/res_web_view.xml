<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="biztech_quick_order" model="ir.ui.view">
        <field name="name">product Website settings</field>
        <field name="model">website.config.settings</field>
        <field name="inherit_id" ref="website.view_website_config_settings"/>
        <field name="arch" type="xml">
            <group name="social" position="after">
                <group string="Product Advance Option">
                    <label for="quick_order"/>
                    <div name="product_option">
                        <div class="oe_inline">
                            <field name="quick_order"/>
                        </div>
                    </div>
                    <label for="title"/>
                    <div name="title">
                        <div class="oe_inline">
                            <field name="title"/>
                        </div>
                    </div>
                    <label for="srt_message"/>
                    <div name="srt_message">
                        <div class="oe_inline">
                            <field name="srt_message"/>
                        </div>
                    </div>
                    <label for="max_item_page"/>
                    <div name="max_item_page">
                        <div class="oe_inline">
                            <field name="max_item_page"/>
                        </div>
                    </div>
                    <label for="multi_list"/>
                    <div name="multi_list">
                        <div class="oe_inline">
                            <field name="multi_list"/>
                        </div>
                    </div>
                </group>
            </group>
        </field>
    </record>
</odoo>